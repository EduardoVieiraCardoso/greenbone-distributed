# ===========================================
# Probe Stack - Environment Configuration
# ===========================================

# Probe Identification
PROBE_ID=probe-01
PROBE_LOCATION=São Paulo, BR
PROBE_TOKEN=your-probe-token

# GVM/OpenVAS credentials
GVM_USERNAME=admin
GVM_PASSWORD=admin

# Central Connection
NATS_URL=nats://your-central-server:4222
NATS_TOKEN=your-nats-token
CENTRAL_WEBHOOK=https://your-central-server:8081/results

# VoidProbe (para IP dinâmico)
VOIDPROBE_SERVER=your-central-server:9000
VOIDPROBE_TOKEN=your-voidprobe-token

# Logging
LOG_LEVEL=INFO

# OpenVAS Settings
AUTO_SYNC=true
HTTPS=true
